module Modules.UserUtils {
    fn make_user(name, age) {
        return {"name": name, "age": age};
    }

    fn age_if_adult(user) {
        let age_opt = user["age"];
        return match age_opt {
            Some(a) -> if a > 17 { Some(a) } else { None },
            None -> None,
            _ -> None,
        };
    }
}
