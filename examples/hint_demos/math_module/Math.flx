// Module: Math
// Provides mathematical utility functions

module Math {
    // Add two numbers
    fn add(a, b) {
        return a + b;
    }

    // Multiply two numbers
    fn multiply(x, y) {
        return x * y;
    }

    // Calculate average of two numbers
    fn average(num1, num2) {
        return (num1 + num2) / 2;
    }
}
