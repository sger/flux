import Math

fn test_add_basic() {
    assert_eq(Math.add(1, 2), 3)
}

fn test_add_negative() {
    assert_eq(Math.add(-5, 3), -2)
}

fn test_subtract() {
    assert_eq(Math.subtract(10, 4), 6)
}

fn test_multiply() {
    assert_eq(Math.multiply(3, 4), 12)
}

fn test_square() {
    assert_eq(Math.square(5), 25)
}

fn test_abs_positive() {
    assert_eq(Math.abs_val(42), 42)
}

fn test_abs_negative() {
    assert_eq(Math.abs_val(-7), 7)
}

fn test_factorial_base() {
    assert_eq(Math.factorial(1), 1)
}

fn test_factorial_recursive() {
    assert_eq(Math.factorial(5), 120)
}

fn test_clamp_within() {
    assert_eq(Math.clamp(5, 0, 10), 5)
}

fn test_clamp_below() {
    assert_eq(Math.clamp(-3, 0, 10), 0)
}

fn test_clamp_above() {
    assert_eq(Math.clamp(15, 0, 10), 10)
}
