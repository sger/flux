module Modules.StringUtils {
    fn wrap(s, prefix, suffix) {
        prefix + s + suffix;
    }

    fn parenthesize(s) {
        "(" + s + ")";
    }

    fn bracket(s) {
        "[" + s + "]";
    }

    fn quote(s) {
        "\"" + s + "\"";
    }

    fn exclaim(s) {
        s + "!";
    }

    fn greet(name) {
        "Hello, " + name;
    }

    fn uppercase_first(s) {
        // Simple simulation - just adds a marker
        "[UPPER]" + s;
    }
}
